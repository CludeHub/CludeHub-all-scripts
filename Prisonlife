local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Prison Life", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
local Tab = Window:MakeTab({
	Name = "Modded Gun",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
Tab:AddButton({
	Name = "Mod M9",
	Callback = function()
local player = game:GetService("Players").LocalPlayer
        local gun = player.Backpack:FindFirstChild("M9")
        local sM = require(gun:FindFirstChild("GunStates"))
        sM["Damage"] = 999
        sM["MaxAmmo"] = 9999991
        sM["StoredAmmo"] = 9999991
        sM["FireRate"] = 0.05
        sM["AmmoPerClip"] = 9999991
        sM["Range"] = 5000
        sM["ReloadTime"] = 0.05
        sM["Bullets"] = 1
        sM["AutoFire"] = true
      		print("button pressed")
  	end    
})
